<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.Cursor?>
<Button xmlns:fx="http://javafx.com/fxml/1" fx:id="answerButton" maxHeight="1.7976931348623157E308"
        maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Button" wrapText="true"
        xmlns="http://javafx.com/javafx/8" fx:controller="org.rijnders.MultiChoiceButtonController"
        onAction="#setAnswerButton">
   <userData>
      <fx:reference source="controller"/>
   </userData>
   <cursor>
      <Cursor fx:constant="HAND"/>
   </cursor>
</Button>
